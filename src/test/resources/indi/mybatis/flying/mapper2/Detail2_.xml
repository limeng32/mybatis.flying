<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="indi.mybatis.flying.mapper2.Detail2Mapper">
	<cache type="org.mybatis.caches.redis.RedisCache" />

	<select id="select" resultMap="result">
		flying#{?}:select
	</select>

	<select id="selectAll" resultMap="result">
		flying:selectAll
	</select>

	<select id="selectOne" resultMap="result">
		flying:selectOne
	</select>

	<select id="count" resultType="int">
		flying:count
	</select>

	<insert id="insert" useGeneratedKeys="true" keyProperty="id">
		flying:insert
	</insert>
	
	<insert id="insertWithoutName" useGeneratedKeys="true" keyProperty="id">
		flying:insert:noName
	</insert>
	
	<insert id="insertWithoutFoo" useGeneratedKeys="true" keyProperty="id">
		flying:insert:noFoo
	</insert>

	<update id="update">
		flying:update
	</update>
	
	<update id="updateWithoutName">
		flying:update:noName
	</update>

	<update id="updatePersistent">
		flying:updatePersistent
	</update>
	
	<update id="updatePersistentWithoutName">
		flying:updatePersistent:noName
	</update>

	<delete id="delete">
		flying:delete
	</delete>

	<resultMap id="result" type="Detail2_" autoMapping="true">
		<id property="id" column="id" />
		<result property="number" column="number_" />
		<association property="loginLogSource2" javaType="LoginLogSource2"
			select="indi.mybatis.flying.mapper2.LoginLogSource2Mapper.select"
			column="loginlog_id" />
	</resultMap>
</mapper>