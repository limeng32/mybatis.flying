<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="indi.mybatis.flying.mapper2.LoginLogSource2Mapper">
	<cache ${mybatis.cache}/>

	<select id="selectWithoutCache" resultMap="result" useCache="false">
		flying#{?}:{"action":"select", "dataSourceId":"smartDataSource2", "connectionCatalog":"testdb2"}
	</select>

	<select id="select" resultMap="result">
		flying#{?}:{"action":"select", "dataSourceId":"smartDataSource2", "connectionCatalog":"testdb2"}
	</select>

	<select id="selectWithoutAccount" resultMap="result">
		flying#{?}:{"action":"select", "ignoreTag":"noAccount"}
	</select>

	<select id="selectAll" resultMap="result">
		flying:{"action":"selectAll"}
	</select>

	<select id="selectOne" resultMap="result">
		flying:{"action":"selectOne"}
	</select>

	<select id="count" resultType="int">
		flying:{"action":"count"}
	</select>

	<insert id="insert" useGeneratedKeys="true" keyProperty="id">
		flying:{"action":"insert"}
	</insert>

	<update id="update">
		flying:{"action":"update"}
	</update>

	<update id="updateNoFlush" flushCache="false">
		flying:{"action":"update"}
	</update>

	<update id="updatePersistent">
		flying:{"action":"updatePersistent"}
	</update>

	<delete id="delete">
		flying:{"action":"delete"}
	</delete>

	<resultMap id="result" type="LoginLogSource2" autoMapping="true">
		<id property="id" column="id" />
		<association property="account" javaType="Account_"
			select="indi.mybatis.flying.mapper.AccountMapper.selectWithoutCache"
			column="accountId" />
	</resultMap>
</mapper>