<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="indi.mybatis.flying.mapper.ProductMapper">
	<cache ${mybatis.cache}/>

	<select id="select" resultMap="result">
		flying#{?}:{"action":"select", "ignoreTag":"select:noPassword"}
	</select>

	<select id="selectOne" resultMap="result">
		flying:{"action":"selectOne"}
	</select>

	<insert id="insert" keyProperty="id">
		flying:{"action":"insert", "keyGeneratorType":"uuid_no_line"}
	</insert>

	<insert id="insert2" keyProperty="id">
		flying:{"action":"insert"}
	</insert>

	<insert id="insert3">
		flying:{"action":"insert", "keyGeneratorType":"ERROR"}
	</insert>

	<insert id="insertMilliSecond">
		flying:{"action":"insert", "keyGeneratorType":"millisecond"}
	</insert>

	<insert id="insertMilliSecond2">
		flying:{"action":"insert", "keyGeneratorType":"millisecond"}
	</insert>

	<insert id="insertSnowFlake">
		flying:{"action":"insert", "keyGeneratorType":"snowflake"}
	</insert>

	<insert id="insertMySnowFlake">
		flying:{"action":"insert", "keyGeneratorType":"indi.mybatis.flying.handlers.MySnowFlakeKeyHandler"}
	</insert>

	<insert id="insertMySnowFlake2">
		flying:{"action":"insert", "keyGeneratorType":"indi.mybatis.flying.handlers.MySnowFlakeKeyHandler2"}
	</insert>

	<insert id="insertAsd">
		flying:{"action":"insert", "keyGeneratorType":"indi.mybatis.flying.keyHandler.AsdKeyGenerator"}
	</insert>

	<insert id="insertDistributedSnowflake">
		flying:{"action":"insert", "keyGeneratorType":"indi.mybatis.flying.keyHandler.DistributedSnowflakeKeyGenerator2"}
	</insert>

	<resultMap id="result" type="Product" autoMapping="true">
		<id property="id" column="id" />
	</resultMap>
</mapper>